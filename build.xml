<project name="vintageJS" default="lint">

    <property name="file" value="src/vintage.js"/>

    <!--  lint (http://www.jslint.com/lint.html) -->
    <target name="lint">
        <!-- ant lint -Dfile=src/cajal.js -->
        <java jar="build/js.jar" fork="true">
            <arg value="build/jslint-check.js" />
            <arg value="${file}" />
        </java>
    </target>

</project>
